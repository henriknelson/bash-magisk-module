# config file for bash-completion
# https://github.com/scop/bash-completion

set (BASH_COMPLETION_VERSION "2.9")

set (BASH_COMPLETION_PREFIX "/system")
set (BASH_COMPLETION_COMPATDIR "/system/etc/bash_completion.d")
set (BASH_COMPLETION_COMPLETIONSDIR "/system/share/bash-completion/completions")
set (BASH_COMPLETION_HELPERSDIR "/system/share/bash-completion/helpers")

set (BASH_COMPLETION_FOUND "TRUE")
